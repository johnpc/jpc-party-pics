{
  "name": "jpc-party-pics",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && npm run spell-check",
    "preview": "vite preview",
    "sandbox": "ampx sandbox --profile personal --once",
    "prod-config": "ampx generate outputs --app-id d131sf2ylw1gxp --branch main --profile personal",
    "prepare": "husky install",
    "spell-check": "cspell \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.5.4",
    "@aws-amplify/ui-react-storage": "^3.3.8",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@tanstack/react-query": "^5.90.10",
    "aws-amplify": "^6.6.5",
    "jszip": "^3.10.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-html5video": "^2.5.1",
    "react-player": "^2.16.0",
    "react-qr-code": "^2.0.15"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.0",
    "@aws-amplify/backend-cli": "^1.2.9",
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "aws-cdk": "^2.162.1",
    "aws-cdk-lib": "^2.162.1",
    "constructs": "^10.4.2",
    "cspell": "^8.15.4",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.json": "prettier --write",
    "*.yml": "prettier --write",
    "*.md": "prettier --write"
  }
